{
    "name": "gabriel403/slim-api",
    "description": "Tool for generating slim api controllers/models",
    "require": {
        "php"              : ">5.6",
        "pimple/pimple"    : "^3.0@dev",
        "symfony/console"  : "2.*",
        "mikey179/vfsStream": "~1.5@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"SlimApi\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApiTest\\": "tests/phpunit/"}
    },
    "bin": [
        "bin/slimapi"
    ]
}
