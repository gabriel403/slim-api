{
    "name": "slimphp-api/slim-api",
    "description": "Tool for generating slim api controllers/models",
    "require": {
        "php": ">5.6",
        "pimple/pimple": "3.*",
        "symfony/console": "2.*",
        "slimphp-api/slim-phinx": "dev-master",
        "slim/slim": "3.*-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mikey179/vfsStream": "1.*",
        "satooshi/php-coveralls": "^0.7.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"SlimApi\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApiTest\\": "tests/phpunit/"}
    },
    "bin": [
        "bin/slimapi"
    ]
}
